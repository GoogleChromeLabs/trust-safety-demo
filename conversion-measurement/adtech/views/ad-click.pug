doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Ad
body(style={"font-family": "sans-serif", "font-size": "12px", "background": "#f6d7ff", "cursor": "pointer"})

div Ad (click source with an `a` element)
span ID: 
span(id="adid")
a(id="ad" href=href attributiondestination=attributiondestination attributionsourceeventid='' attributionreportto=attributionreportto attributionexpiry='864000000' target='_blank')
  img(width='180' src='/shoes07.png' alt='shoes 07')

script.
  // Define the source event ID
  const attributionsourceeventid=`${Math.floor(Math.random() * 1000000000000000)}`;
  // Set the source event ID
  const adEl = document.getElementById('ad');
  adEl.setAttribute("attributionsourceeventid", attributionsourceeventid);
  // Display the source event ID in the UI for demo purposes
  const adIdEl = document.getElementById('adid');
  adIdEl.innerText = attributionsourceeventid;

  
